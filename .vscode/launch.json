{
    "configurations": [
        {
            "type": "debugpy",
            "request": "launch",
            "name": "API",
            "program": "${workspaceFolder}/backend/api/main.py",
            "args": [
                "-m", "uvicorn", "backend.api.main:app", "--reload"
            ],
            "envFile": "${workspaceFolder}/backend/.env",
            "env": {
                "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/backend"
            }
        },
        {
            "type": "debugpy",
            "request": "launch",
            "name": "Teste",
            "program": "${workspaceFolder}/backend/teste/main.py",
            "args": [
                "--reload"
            ],
            "envFile": "${workspaceFolder}/backend/.env",
            "env": {
                "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/backend"
            }
        }
    ]
}
